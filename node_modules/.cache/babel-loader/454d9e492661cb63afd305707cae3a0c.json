,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,KAAA,CAAA,QAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,SAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AAEA,UAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,OAAA,EAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,QAAA,EAAA,IAHA;AAIA,YAAA,WAAA,EAAA;AAJA,WAAA;AAMA,SAvBA;AAwBA,OAzBA,MAyBA;AACA,aAAA,OAAA,GAAA,KAAA;AACA,aAAA,IAAA,GAAA,KAAA;AAEA,aAAA,MAAA,CAAA,KAAA,CAAA,sBAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA,WAFA;AAGA,UAAA,QAAA,EAAA,IAHA;AAIA,UAAA,WAAA,EAAA;AAJA,SAAA;AAMA;AACA;AA1CA,GAfA;AA2DA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,OAAA,GAAA,QAAA,GAAA,QAAA;AACA,KAHA;AAIA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,IAAA,GAAA,QAAA,GAAA,QAAA;AACA;AANA;AA3DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"Vat\">\r\n    <Navbar />\r\n    <div class=\"container\">\r\n      <h3>VAT</h3>\r\n      <hr />\r\n      <div class=\"row mt-2\">\r\n        <div class=\"col\">\r\n          <form class=\"mt-4\" v-on:submit.prevent>\r\n            <div class=\"form-group\">\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                v-model=\"bookcode\"\r\n                placeholder=\"Bookcode...\"\r\n              />\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col col-md-1\">\r\n                <button type=\"submit\" class=\"btn btn-success\" @click=\"runTask\">\r\n                  Submit\r\n                </button>\r\n              </div>\r\n              <div class=\"col col-md-11\">\r\n                <b-icon\r\n                  icon=\"three-dots\"\r\n                  animation=\"cylon\"\r\n                  font-scale=\"3\"\r\n                  variant=\"success\"\r\n                  :class=\"load\"\r\n                />\r\n              </div>\r\n            </div>\r\n\r\n            <vatdetail \r\n                :resp=\"resp\" \r\n                :hide=\"hide\" \r\n                :flex=\"flex\" \r\n                :book=\"book\" \r\n            />\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport vatdetail from \"@/components/VatDetail.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Vat\",\r\n  components: {\r\n    Navbar,\r\n    vatdetail,\r\n  },\r\n  data() {\r\n    return {\r\n      bookcode: null,\r\n      book: null,\r\n      resp: [],\r\n      loading: false,\r\n      hide: false,\r\n    };\r\n  },\r\n  methods: {\r\n    setResp(data) {\r\n      this.resp = data;\r\n    },\r\n    runTask() {\r\n      this.loading = true;\r\n      if (this.bookcode) {\r\n        axios\r\n          .get(\r\n            \"https://api2.lippo.homes//api/services/app/PSASSchedule/GetPSASScheduleHeader?bookCode=\" +\r\n              this.bookcode\r\n          )\r\n          .then((response) => {\r\n            this.setResp(response.data.result);\r\n            this.book = this.bookcode;\r\n            this.bookcode = null;\r\n            this.loading = false;\r\n            this.hide = true;\r\n          })\r\n          .catch((error) => {\r\n            this.loading = false;\r\n            this.hide = false\r\n            console.log(error);\r\n            \r\n            this.$toast.error(error.response.data.error.message, {\r\n                type: \"error\",\r\n                position: \"top-right\",\r\n                duration: 3000,\r\n                dismissible: true,\r\n            });\r\n          });\r\n      } else {\r\n        this.loading = false;\r\n        this.hide = false\r\n\r\n        this.$toast.error(\"Bookcode harus diisi\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    load: function () {\r\n      return this.loading ? \"d-flex\" : \"d-none\";\r\n    },\r\n    flex: function () {\r\n      return this.hide ? \"d-flex\" : \"d-none\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views"}]}