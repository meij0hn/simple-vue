r />\r\n    <div class=\"container\">\r\n        <h3>Cek Payment Order</h3>\r\n        <hr />\r\n        <div class=\"row mt-2\">\r\n            <div class=\"col\">\r\n                <form class=\"mt-4\" v-on:submit.prevent>\r\n                    <div class=\"form-group\">\r\n                    <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        v-model=\"ordercode\"\r\n                        placeholder=\"Ordercode...\"\r\n                    />\r\n                    </div>\r\n\r\n                    <button type=\"submit\" class=\"btn btn-success\" @click=\"runTask\">\r\n                    Submit\r\n                    </button>\r\n                </form>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"row mt-5\">\r\n            <div class=\"col\">\r\n                <table class=\"table table-borderless\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"5%\">Ordercode</td>\r\n                            <td width=\"1%\">:</td>\r\n                            <td>Answer</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>Amount</td>\r\n                            <td>:</td>\r\n                            <td>3000000</td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n            \r\n        </div>\r\n        \r\n        \r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"cekpaymentorder\",\r\n  components: {\r\n    Navbar,\r\n  },\r\n  data() {\r\n    return {\r\n      ordercode: null,\r\n      dataResult: null,\r\n    }\r\n  },\r\n  methods: {\r\n      runTask() {\r\n          if(this.ordercode) {\r\n              axios\r\n                .post(\"https://api.lippo.