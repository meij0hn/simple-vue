A,CAAA,KAAA;AACA,WAXA;AAYA,SA5BA,EA6BA,KA7BA,CA6BA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SA/BA;AAgCA,OAjCA,MAiCA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,8BAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA,WAFA;AAGA,UAAA,QAAA,EAAA,IAHA;AAIA,UAAA,WAAA,EAAA;AAJA,SAAA;AAMA;AACA,KA3CA;AA4CA,IAAA,sBA5CA,oCA4CA;AACA,UAAA,KAAA,MAAA,IAAA,CAAA,IAAA,KAAA,MAAA,GAAA,EAAA,EAAA;AACA,aAAA,YAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,mBAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA,WAFA;AAGA,UAAA,QAAA,EAAA,IAHA;AAIA,UAAA,WAAA,EAAA;AAJA,SAAA;AAMA;AACA;AAvDA;AATA,CAAA","sourcesContent":["<template>\r\n  <div class=\"row\" :class=\"flex2\">\r\n    <div class=\"col\">\r\n      <form v-on:submit.prevent>\r\n        <div class=\"form-group\">\r\n          <div class=\"row\">\r\n            <div class=\"col col-md-6\">\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                v-model=\"pctTax\"\r\n                placeholder=\"New PctTax...\"\r\n              />\r\n            </div>\r\n            <div class=\"col col-md-6\">\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                v-model=\"tiket\"\r\n                placeholder=\"No Tiket...\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"row mt-2\">\r\n            <div class=\"col\">\r\n              <button\r\n                type=\"submit\"\r\n                class=\"btn btn-success\"\r\n                @click=\"runUpdateTaxValidation\"\r\n              >\r\n                Update Tax\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"vatupdate\",\r\n  data() {\r\n    return {\r\n      pctTax: \"\",\r\n      tiket: \"\",\r\n    };\r\n  },\r\n  props: [\"book\", \"flex2\", \"hide2\", \"resp\"],\r\n  methods: {\r\n    runUpdateTax() {\r\n      if (this.pctTax && this.tiket) {\r\n        axios\r\n          .post(\"http://api.mrmjohn.com/services/api/updateVat\", {\r\n            bookcode: this.book,\r\n            pctTax: parseFloat(this.pctTax),\r\n            tiket: this.tiket,\r\n          })\r\n          .then((response) => {\r\n            console.log(response);\r\n\r\n            this.$toast.success(\"Success Update PctTax\", {\r\n              type: \"success\",\r\n              position: \"top-right\",\r\n              duration: 3000,\r\n              dismissible: true,\r\n            });\r\n\r\n            axios\r\n              .get(\r\n                \"https://api2.lippo.homes//api/services/app/PSASSchedule/GetPSASScheduleHeader?bookCode=\" +\r\n                  this.book\r\n              )\r\n              .then(() => {\r\n                this.$router.go('/vat')\r\n\r\n              })\r\n              .catch((error) => {\r\n                console.log(error);\r\n              });\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        this.$toast.error(\"PctTax or Tiket harus diiisi\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n    runUpdateTaxValidation() {\r\n      if (this.pctTax >= 0 && this.pctTax < 10) {\r\n        this.runUpdateTax();\r\n      } else {\r\n        this.$toast.error(\"Isi pctTax 0 - 10\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components"}]}