bmit\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row mt-5\" :class=\"flex\">\r\n        <div class=\"col\">\r\n          <table class=\"table table-borderless\">\r\n            <tbody>\r\n              <tr>\r\n                <td width=\"13%\">Ordercode</td>\r\n                <td width=\"1%\">:</td>\r\n                <td>{{ dataResult.order_id }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Amount</td>\r\n                <td>:</td>\r\n                <td>Rp {{ dataResult.gross_amount | currency }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Payment Type</td>\r\n                <td>:</td>\r\n                <td>{{ dataResult.payment_type }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Transaction Time</td>\r\n                <td>:</td>\r\n                <td>{{ dataResult.transaction_time | formatDate }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"cekpaymentorder\",\r\n  components: {\r\n    Navbar,\r\n  },\r\n  data() {\r\n    return {\r\n      ordercode: null,\r\n      dataResult: \"\",\r\n      hide: false,\r\n    };\r\n  },\r\n  methods: {\r\n    runTask() {\r\n      if (this.ordercode) {\r\n        axios\r\n          .post(\r\n            \"https://api.lippo.homes/api/services/app/PaymentOB/CheckPaymentStatus?orderCode=\" +\r\n              this.ordercode\r\n          )\r\n          .then((response) => {\r\n            this.dataResult = response.data.result;\r\n            if (this.dataResult.status_code == 404) {\r\n              this.hide = false;\r\n              this.$toast.error(this.dataResult.status_message, {\r\n                type: \"error\",\r\n                position: \"top-right\",\r\n                duration: 3000,\r\n                dismissible: true,\r\n              });\r\n              \r\n            \r\n            } else {\r\n              this.hide = true;\r\n              this.ordercode = null;\r\n            }\r\n          })\r\n          .catch((error) => console.log(error));\r\n      } else {\r\n        this.$toast.error(\"Ordercode harus diisi\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    flex: function () {\r\n      return this.hide ? \"d-flex\" : \"d-none\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}